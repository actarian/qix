{
  "targets": {
    "browser": {
      "compile": [
        {
          "input": "src/css/main.scss",
          "output": "docs/css/main.css",
          "minify": false
        },
        {
          "input": "src/js/main.js",
          "output": "docs/js/main.js",
          "minify": false
        }
      ],
      "bundle": [
        {
          "input": [
            "src/js/howler.js"
          ],
          "output": "docs/js/vendors.js",
          "minify": false
        }
      ]
    }
  },
  "tfs": false,
  "server": {
    "root": "./docs",
    "path": "/qix/",
    "host": "0.0.0.0",
    "port": 40111,
    "log": false
  }
}